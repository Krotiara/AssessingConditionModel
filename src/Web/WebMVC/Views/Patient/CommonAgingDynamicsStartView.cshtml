@{
    Layout = "~/Views/Shared/_MedicLayout.cshtml";
}

<html>
    <head></head>
    <body>
        <div id="commonAgingDynamics">
            <table style="width:30%; table-layout: fixed;" class="table table-sm">
                <tr>
                    <th colspan="3"><p>Общая динамика:</p></th>
                    <th colspan="2"><input style="width:100%" type="date" id="dtCommon1" /></th>
                    <th colspan="1"><p>-</p></th>
                    <th colspan="2"><input style="width:100%" type="date" id="dtCommon2" /></th>
                    <th colspan="2"><button style="width:100%" onclick="GetAgingDynamics()">Показать</button></th>
                </tr>
            </table>
        </div>
        <div id="result"></div>
    </body>
</html>

<script>
    function GetAgingDynamics() {
        var startTime = document.getElementById("dtCommon1").value;
        var endTime = document.getElementById("dtCommon2").value;
        $.ajax({
            url: '@Url.Action("GetAgingDynamics", "Patient")',
            data: { startTimestamp: startTime, endTimestamp: endTime },
            type: 'get',
            cache: false,
            async: true,
            success: function (result) {
                $('#result').html(result);
            }
        });
    }
</script>