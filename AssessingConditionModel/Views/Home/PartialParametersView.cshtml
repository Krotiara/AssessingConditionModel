 <table class="table table-bordered table-hover">
        @foreach(KeyValuePair<string, string> pair in Model.GetDisplayAttributes<object>())
            {
             <tr>
                 <th>
                     <p>@pair.Key</p>
                 </th>
                 <th>
                      <p>@pair.Value</p>
                 </th>
             </tr>
        }
</table>
